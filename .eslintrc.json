{
    "env": {
        "es2021": true,
         "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
    "accessor-pairs": "off",
    "no-console": "error"
    }
}
